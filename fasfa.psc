Proceso sin_titulo
	Definir a, b , c, d, e ,f ,g ,h , i Como real;
	Escribir "Ingrese la primera nota";
	Leer a;
	Escribir "Ingrese la segunda nota";
	Leer b;
	Escribir "Ingrese la tercera nota";
	Leer c;
	Escribir "ingrese la nota del examen final";
	Leer d;
	
	e <- a * 0.10;
	f <- b * 0.20;
	g <- c * 0.30;
	h <- d * 0.40;
	i <- e + f + g + h;
	
	Escribir "El promedio de la notas es: ", i;
FinProceso
